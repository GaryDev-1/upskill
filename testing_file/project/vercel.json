{
  "name": "upskill-frontend",
  "version": 2,
  "builds": [
    {
      "src": "testing_file/project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/testing_file/project/$1"
    }
  ],
  "functions": {
    "testing_file/project/src/**": {
      "runtime": "nodejs18.x"
    }
  },
  "buildCommand": "cd testing_file/project && npm run build",
  "outputDirectory": "testing_file/project/dist",
  "installCommand": "cd testing_file/project && npm install"
}